    "@awesome-cordova-plugins/photo-library": "^6.4.0",
    "@awesome-cordova-plugins/social-sharing": "^6.3.0",
    "@capacitor-community/apple-sign-in": "^4.0.0",
    "@capacitor-community/fcm": "^5.0.2",
    "@capacitor/android": "^5.0.0",
    "@capacitor/app": "^5.0.0",
    "@capacitor/browser": "^5.0.0",
    "@capacitor/camera": "^5.0.0",
    "@capacitor/clipboard": "^5.0.0",
    "@capacitor/core": "^5.0.0",
