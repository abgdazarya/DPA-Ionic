{
  "name": "MyDPA",
  "version": "1.0.0",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "ng": "ng",
    "start": "node config && ng serve",
    "build": "node config && BUILD_ENV=${BUILD_ENV:-local} ng build --configuration=$BUILD_ENV",
    "watch": "node config && ng build --watch --configuration development",
    "test": "node config && ng test",
    "lint": "node config && ng lint",
    "emulate:android:live": "node config && ionic cap run android -l --external",
    "build:android": "node config && ionic build && ionic cap copy && ionic cap sync && ionic cap open android",
    "build:android:prod": "node config && ionic build --production && ionic cap copy && ionic cap sync && ionic cap open android",
    "build:ios": "node config && ionic build && ionic cap copy && ionic cap sync && ionic cap open ios",
    "build:ios:prod": "node config && ionic build --production && ionic cap copy && ionic cap sync && ionic cap open ios",
    "gradlew:dev": "node config && npm install @capacitor/device@4.1.0 @capacitor/clipboard@4.1.0 @codetrix-studio/capacitor-google-auth@3.2.2 && ionic build && ionic cap copy && ionic cap sync && cd android && gradlew clean && gradlew assembleRelease",
    "gradlew:prod": "node config && npm install @capacitor/device@4.1.0 @capacitor/clipboard@4.1.0 @codetrix-studio/capacitor-google-auth@3.2.2 && ionic build --production && ionic cap copy && ionic cap sync && cd android && gradlew clean && gradlew assembleRelease --stacktrace && gradlew bundleRelease --stacktrace",
    "icon:andorid": "npx @capacitor/assets generate --assetPath='./src/assets/' --android",
    "icon:ios": "npx @capacitor/assets generate --assetPath='./src/assets/' --ios",
    "icon:pwa": "npx @capacitor/assets generate --assetPath='./src/assets/' --pwa",
    "set-cert:ios": "mkdir -p ios/App/App/www/certificates && cp src/certificates/*.cer ios/App/App/www/certificates && ruby addCertToXcodeProj.rb"
  },
  "overrides": {
    "chokidar": "3.5.3",
    "glob-parent": "6.0.2",
    "semver": "7.5.3",
    "sharp": "0.32.6",
    "xml2js": "0.5.0"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/fire": "^7.6.1",
    "@angular/forms": "^15.0.0",
